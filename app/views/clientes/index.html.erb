<p style="color: green"><%= notice %></p>

<h1>Clientes</h1>

<div id="clientes">
  <% @clientes.each do |cliente| %>
    <%= render cliente %>
    <p>
      <%= link_to "Show this cliente", cliente, class: "button" %>
    </p>
  <% end %>
</div>

<%= link_to "New cliente", new_cliente_path, class: "button" %>
